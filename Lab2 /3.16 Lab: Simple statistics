num1 = float(input())
num2 = float(input())
num3 = float(input())
num4 = float(input())

''' Type your code here. '''

mult = num1 * num2 * num3 * num4
avg = (num1 + num2 + num3 + num4) / 4

int_mult = round(num1 * num2 * num3 * num4)
int_avg = int(round((num1 + num2 + num3 + num4) / 4))

print(f'{int_mult:.0f} {int_avg:.0f}')
print(f'{mult:.3f} {avg:.3f}')
